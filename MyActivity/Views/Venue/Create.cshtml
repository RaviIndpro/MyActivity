@model Venue

<form method ="post">
   <h2>
       Create a new Venue
   </h2>
   <label asp-for ="StadiumName"></label>
   <input asp-for ="StadiumName" class ="form-control" />
   <span asp-validation-for="StadiumName" class ="text-danger">

   </span><br />
   <button type ="submit" class= "btn btn-primary">
       Create
   </button>
   <a asp-controller="Venue" asp-action="Index" class = "btn btn-secondary">
       Back
   </a>
</form>
